//write a program for duplicatein given array
#include <stdio.h>

int main() {
    int a[8], i, j;
    printf("Enter array elements:\n");
    for (i = 0; i < 5; i++) {
        scanf("%d", &a[i]);
    }

    printf("Duplicate numbers:\n");
    for (i = 0; i < 5; i++) {
        for (j = i + 1; j < 5; j++) {
            if (a[i] == a[j]) {
                printf("%d ", a[j]);
            }
        }
    }
    return 0;
}
