// multiplication of array
#include <stdio.h>

int main()
{
    int a[2][3],b[2][3],c[2][3],i,j;
    printf("enter element of aarray1");
    for(i=0;i<2;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&a[i][j]);
        }
    }
     printf("enter the value of array2");
    for(i=0;i<2;i++)
    {
        for(j=0;j<3;j++)
        {
            scanf("%d",&b[i][j]);
        }
    }
     for(i=0;i<2;i++)
    {
        for(j=0;j<3;j++)
        {
            c[i][j]=a[i][j]*b[2][3];
            printf("%d\t",c[i][j]);
        }
        printf("\n");
    }

    return 0;
}
