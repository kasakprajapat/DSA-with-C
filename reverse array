// write code for reverse the array
#include <stdio.h>

int main() {
    int a[8],i,j;
    printf("array element is:");
    for(int i=0;i<8;i++)
    {
    scanf("%d",&a[i]);
    }
        printf("Reversed elements are:\n");
    for (j = 7; j >= 0; j--) {
        printf("%d ", a[j]);
    }
    return 0;
}
