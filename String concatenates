//Wirte a code for string concatenates 
#include <stdio.h>
#include <string.h>
int main() 
{
    char str1[50],str2[25];
        printf("\n enter the first sting");
        gets(str1);
    //use gets for print whole string
        printf("\n enter the second string");
        gets(str2);
        strcat(str1,str2);
        printf("\n output string after concatenations : %s",str1);
        printf("\nlength of string is: %d",strlen(str1));
    return 0;
}
